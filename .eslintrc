{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-console": 2,
    "no-debugger": 2,
    "semi": 0,
    "no-param-reassign": 0,
    "max-len": ["error", 150, 2],
    "no-unused-vars": ["error", {
      "vars": "all",
      "args": "none",
      "ignoreRestSiblings": true
    }]
  }
}
